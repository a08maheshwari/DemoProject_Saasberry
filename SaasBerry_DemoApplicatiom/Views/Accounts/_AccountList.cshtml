<div class="col-row">

    @if (ViewBag.Alist.Count > 0)
    {
    <table class="table table-striped table-hover cursor" id="tblAccounts">
        <thead>
            <tr>
                <th width="9%">First Name</th>
                <th width="9%">Last Name </th>
                <th width="9%">Mobile</th>
                <th width="10%">Email</th>
                <th width="9%">Address 1</th>
                <th width="9%">Address 2</th>
                <th width="9%">City</th>
                <th width="9%">Postal Code</th>
                <th width="9%">Country</th>
                <th width="9%">Roles</th>
                <th width="9%">StartDate</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var i in ViewBag.Alist)
            {
                <tr>
            <td>@i.FirstName</td>
            <td>@i.LastName</td>
            <td>@i.Mobile</td>
            <td>@i.Email</td>
            <td>@i.Address1</td>
            <td>@i.Address2</td>
            <td>@i.City</td>
            <td>@i.PostalCode</td>
            <td>@i.Country</td>
            <td>@i.Roles</td>
            <td>@i.StartDate</td>
            </tr>
        }
            </tbody>

        </table>
    }
    else
    {
        <div class="bs-callout bs-callout-info text-center">
            <i class="fa fa-folder-open"></i>
            <br>
            No records to display
        </div>
    }
    <div class="text-center col-row mar-t-10">
        <input type="button" value="Load More" id="btnLoadmore" style="display:none;" class="btn btn-gray-l" />
    </div>
</div>
